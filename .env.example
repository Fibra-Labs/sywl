DATABASE_URL=file:local.db
SPOTIFY_CLIENT_ID=your client
SPOTIFY_CLIENT_SECRET=your secret
SPOTIFY_REDIRECT_URI=https://127.0.0.1:5173/login/spotify/callback

# AI Provider Configuration (groq or gemini)
AI_PROVIDER=groq
GROQ_API_KEY=your_groq_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

ORIGIN=https://localhost:5173

# OpenTelemetry / Signoz Configuration
OTEL_SERVICE_NAME=sywl
HOST_NAME=my-vps
OTEL_EXPORTER_OTLP_ENDPOINT=https://otel-http.fibra.dev
# OTEL_EXPORTER_OTLP_HEADERS=signoz-ingestion-key=YOUR_KEY  # Only for Signoz Cloud, not self-hosted
OTEL_LOG_LEVEL=debug
OTEL_TRACES_EXPORTER=otlp
OTEL_NODE_RESOURCE_DETECTORS=env,host,os

# Note: The OTEL_EXPORTER_OTLP_ENDPOINT is used for traces, metrics, and logs
# Paths are automatically appended: /v1/traces, /v1/metrics, /v1/logs
# Metrics are exported every 60 seconds by default
