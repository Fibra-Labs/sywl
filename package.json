{
  "name": "songs-you-will-love",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--require ./instrumentation.cjs' vite dev --host",
    "build": "vite build",
    "preview": "NODE_OPTIONS='--require ./instrumentation.cjs' vite preview",
    "start": "NODE_OPTIONS='--require ./instrumentation.cjs' node build/index.js",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@libsql/client": "^0.7.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.211.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.211.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/instrumentation-winston": "^0.55.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-logs": "^0.211.0",
    "@opentelemetry/sdk-metrics": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@opentelemetry/winston-transport": "^0.21.0",
    "@types/marked": "^5.0.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.32.1",
    "groq-sdk": "^0.37.0",
    "marked": "^16.2.1",
    "svelte-sonner": "^1.0.7",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@inlang/paraglide-js": "^2.0.0",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-node": "^5.5.2",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@types/spotify-api": "^0.0.25",
    "@vitest/browser": "^3.2.3",
    "drizzle-kit": "^0.23.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "playwright": "^1.53.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^3.2.3",
    "vitest-browser-svelte": "^0.1.0"
  }
}
